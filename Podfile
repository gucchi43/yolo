target 'masterch' do
  pod 'NCMB', :git => 'https://github.com/NIFTYCloud-mbaas/ncmb_ios.git'
 
  pod 'FBSDKCoreKit', '~> 4.1.0'
  pod 'FBSDKLoginKit', '~> 4.1.0'

  pod 'Fabric'
  pod 'TwitterKit', '~> 2.1.1'
  pod 'TwitterCore', '~>2.1.1'

  pod 'Crashlytics'

  pod 'RMUniversalAlert'

end
